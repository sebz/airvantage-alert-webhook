{
    "env": {
        "es6": true,
        "node": true
    },
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "plugins": [
        "babel"
    ],
    "rules": {
        "indent": [
            "error",
            4,
            {
                "MemberExpression": 4
            }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "quotes": [
            "error",
            "double"
        ],
        "semi": [
            "error",
            "always"
        ],
        "arrow-parens": 0,
        "babel/arrow-parens": 0
    }
}
